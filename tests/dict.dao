func testdict() {
	a = dict {
		a = 1,
		b = 2,
		c = "sea, see",
		d = "dee, di"
	};
	b = dict {
		a = 100, b = 200, e = list {1,2,3}
	};
	println("a = ", a);
	println("b = ", b);
	println("a|b = ", a|b);
	println("a&b = ", a&b);
	println("a^b = ", a^b);
	println("a-b = ", a-b);
}
testdict();

