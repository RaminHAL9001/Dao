func printArray(a) {
	i = 0;
	for x in a {
		println(i, ": ", x); i += 1;
	}
}

//: init array
//: 0: 1
//: 1: 2
//: 2: 5
//: 3: (THREE, SIR!)
//: 4: 3
//: 5: Five is right out!
println("init array");
a = Array{ 1, 2, 5, "(THREE, SIR!)", 3, "Five is right out!" };
printArray(a);

//: index array
//: (THREE, SIR!)
//: Five is right out!
println("index array");
println(a[3], "\n", a[5]);

//: update array
//: 0: 1
//: 1: two
//: 2: 5
//: 3: (THREE, SIR!)
//: 4: 3
//: 5: 5
println("update array");
a[1] = "two";
a[5] = 5;
printArray(a);

//: iterate update array
//: 0: 0
//: 1: 1
//: 2: 4
//: 3: 3
//: 4: 2
//: 5: 4
println("iterate update array");
i = 0;
for x in a {
	if(Int == typeof(x)) { x -= 1; } else { x = i; }
	i += 1;
}
printArray(a);

