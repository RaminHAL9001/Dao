n = 55555;

func checkValue()  { return (n%2 == 0); }
func doDivide()    { n /= 2; }
func doMult3Add1() { n *= 3; n += 1; }

func checkContinue() {
	if(n <= 1) { println(1); return false; }
	else if(checkValue()) { doDivide(); }
	else { doMult3Add1(); }
	println(n);
	return true;
}

func run() {
	println(n);
	while(checkContinue()) {}
}

run();
