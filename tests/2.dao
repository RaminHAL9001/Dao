string.compare = approximate;

BEGIN {
	$comma = list {};
	comma.count = 0;
}

rule "$*, $*" {
	a = comma.count;
	if(a != 0) { return; }
	comma.count = a + 1;
	a = list {
		"1 = " + join("", $1),
		"2 = " + join("", $2) + "\n"
	};
	$comma += a;
}

END {
	a = 1;
	for x in @$comma {
		print(a+": "+x);
		a = a + 1;
	}
}

